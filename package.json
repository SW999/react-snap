  "license": "MIT",
  "repository": "stereobooster/react-snap",
  "engines": {
    "node": ">= 14.1.0"
  },
  "dependencies": {
    "clean-css": "5.3.1",
    "express": "4.18.2",
    "express-history-api-fallback": "2.2.1",
    "highland": "2.13.5",
    "html-minifier": "4.0.0",
    "minimalcss": "0.11.3",
    "mkdirp": "1.0.4",
    "puppeteer": "19.0.0",
    "serve-static": "1.15.0",
    "sourcemapped-stacktrace-node": "2.1.8"
  },
  "devDependencies": {
    "dev-null-stream": "0.0.1",
